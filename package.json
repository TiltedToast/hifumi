{
    "name": "hifumi-js",
    "version": "1.0.0",
    "type": "module",
    "dependencies": {
        "@discordjs/builders": "^1.6.1",
        "@discordjs/rest": "^1.7.0",
        "@planetscale/database": "^1.7.0",
        "bufferutil": "^4.0.7",
        "canvas": "^2.11.2",
        "discord-api-types": "^0.37.38",
        "discord.js": "^14.9.0",
        "dotenv": "^16.0.3",
        "drizzle-orm": "^0.23.13",
        "fuse.js": "^6.6.2",
        "lint-staged": "^13.2.1",
        "mathjs": "^11.8.0",
        "node-fetch": "^3.3.1",
        "path": "^0.12.7",
        "qrcode": "^1.5.1",
        "snoowrap": "^1.23.0",
        "strftime": "^0.10.1",
        "utf-8-validate": "^5.0.10",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@types/node": "^18.15.11",
        "@types/qrcode": "^1.5.0",
        "@types/request": "^2.48.8",
        "@types/strftime": "^0.9.4",
        "@typescript-eslint/eslint-plugin": "^5.58.0",
        "@typescript-eslint/parser": "^5.58.0",
        "drizzle-kit": "^0.17.4",
        "eslint": "^8.38.0",
        "eslint-cli": "^1.1.1",
        "husky": "^8.0.3",
        "prettier": "^2.8.7",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.4"
    },
    "scripts": {
        "start": "tsc && pm2 start ./dist/app.js --name hifumi --time",
        "stop": "pm2 delete hifumi",
        "restart": "tsc && pm2 reload hifumi",
        "docker-build": "tsc && node ./dist/app.js",
        "lint": "eslint .",
        "prettier": "prettier --config .prettierrc --check ./src",
        "prepare": "husky install",
        "deploy": "tsc && node ./dist/deploy-commands.js",
        "build": "tsc",
        "test": "pnpm run lint && pnpm run prettier",
        "mg:gen": "drizzle-kit generate:mysql --out migrations --schema src/db/schema.ts",
        "mg:up": "drizzle-kit up:mysql --out migrations",
        "deploy-dev": "tsc && node ./dist/deploy-commands.js --guild 655484563077988385",
        "clear-test-commands": "tsc && node ./dist/deploy-commands.js --guild 655484563077988385 --clear"
    },
    "lint-staged": {
        "*.{js,ts,json,css,md,yml}": "prettier --config .prettierrc --write"
    }
}
