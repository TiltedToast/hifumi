{
    "name": "hifumi-js",
    "version": "1.0.0",
    "type": "module",
    "dependencies": {
        "@discordjs/builders": "^1.5.0",
        "@discordjs/rest": "^1.6.0",
        "bufferutil": "^4.0.7",
        "canvas": "^2.11.0",
        "discord-api-types": "^0.37.36",
        "discord.js": "^14.8.0",
        "dotenv": "^16.0.3",
        "drizzle-orm": "^0.23.2",
        "erlpack": "github:discord/erlpack",
        "formdata-node": "^4.4.1",
        "fuse.js": "^6.6.2",
        "mathjs": "^11.7.0",
        "mongodb": "^5.1.0",
        "mysql2": "^3.2.0",
        "node-fetch": "^3.3.1",
        "path": "^0.12.7",
        "qrcode": "^1.5.1",
        "snoowrap": "^1.23.0",
        "strftime": "^0.10.1",
        "utf-8-validate": "^5.0.10",
        "zlib-sync": "^0.1.7",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@types/qrcode": "^1.5.0",
        "@types/request": "^2.48.8",
        "@types/strftime": "^0.9.4",
        "@typescript-eslint/eslint-plugin": "^5.55.0",
        "@typescript-eslint/parser": "^5.55.0",
        "drizzle-kit": "^0.17.1",
        "eslint": "^8.36.0",
        "eslint-cli": "^1.1.1",
        "husky": "^8.0.3",
        "lint-staged": "^13.2.0",
        "prettier": "^2.8.4",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.2"
    },
    "scripts": {
        "start": "tsc && pm2 start ./dist/app.js --name hifumi --time",
        "stop": "pm2 delete hifumi",
        "restart": "tsc && pm2 reload hifumi",
        "docker-build": "tsc && node ./dist/app.js",
        "lint": "eslint ./src",
        "prettier": "prettier --config .prettierrc --check ./src",
        "prepare": "husky install",
        "deploy": "tsc && node ./dist/deploy-commands.js",
        "build": "tsc",
        "test": "npm run lint && npm run prettier",
        "migrate": "drizzle-kit generate:mysql --out migrations --schema src/db/schema.ts",
        "migrate:up": "drizzle-kit up:mysql --out migrations",
        "deploy-dev": "tsc && node ./dist/deploy-commands.js --guild 655484563077988385",
        "clear-test-commands": "tsc && node ./dist/deploy-commands.js --guild 655484563077988385 --clear"
    },
    "lint-staged": {
        "*.{js,ts}": "eslint --cache",
        "*.{js,ts,json,css,md}": "prettier --config .prettierrc --write"
    },
    "packageManager": "pnpm@7.29.3"
}
